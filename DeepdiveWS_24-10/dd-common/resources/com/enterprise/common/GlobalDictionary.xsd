	<xsd:simpleType name="CellphoneNumber">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="|0[78][2347][0-9]{7}"/>
		</xsd:restriction>
	</xsd:simpleType>
            
    <xsd:simpleType name="Alpha_1_255">
